    "docs": "documentation-readme -s \"Botmaster Ware\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botmasterai/botmaster-enrich.git"
  },
  "keywords": [
    "chatbot",
    "bot",
    "chat",
    "pipeline",
    "context"
  ],
