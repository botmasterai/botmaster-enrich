  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botmasterai/botmaster-enrich/issues"
  },
  "homepage": "https://github.com/botmasterai/botmaster-enrich#readme",
  "dependencies": {
    "async": "^2.1.4",
    "node-cache": "^4.1.1",
    "ramda": "^0.23.0"
  },
  "peerDependencies": {
    "botmaster": "^2.0.0"
  },
  "devDependencies": {
    "botmaster": "git+https://github.com/botmasterai/botmaster.git",
    "botmaster-test": "0.0.2",
